{
  "name": "newkit",
  "version": "0.0.1",
  "description": "Newkit based angular2",
  "main": "index.html",
  "scripts": {
    "start": "node build/server.js",
    "dev:vendor": "gulp build:vendor --gulpfile build/gulpfile.js --cwd ./ & gulp build:nk-thirdparty --gulpfile build/gulpfile.js --cwd ./",
    "dev": "gulp --gulpfile build/gulpfile.js --cwd ./",
    "dev:newkit": "gulp build:newkit --gulpfile build/gulpfile.js --cwd ./",
    "dev:nk-core": "gulp build:nk-core --gulpfile build/gulpfile.js --cwd ./",
    "dev:nk-shell": "gulp build:nk-shell --gulpfile build/gulpfile.js --cwd ./",
    "dev:modules": "gulp build:modules --gulpfile build/gulpfile.js --cwd ./",
    "serve": "gulp serve --gulpfile build/gulpfile.js --cwd ./",
    "watch": "gulp watch --gulpfile build/gulpfile.js --cwd ./",
    "release": "gulp release --gulpfile build/gulpfile.js --cwd ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "http://trgit2/backend_framework/newkit.git"
