    "dev:modules": "gulp4 build:modules --gulpfile build/gulpfile.js --cwd ./",
    "dev": "gulp4 build --gulpfile build/gulpfile.js --cwd ./",
    "serve": "gulp4 serve --gulpfile build/gulpfile.js --cwd ./",
    "release": "gulp4 release --gulpfile build/gulpfile.js --cwd ./",
    "test:e2e": "testcafe chrome test/e2e || true",
    "test:unit": "testcafe chrome test/e2e",
    "test": "npm run test:e2e & npm run test:unit",
    "lint": "tslint -c tslint.json 'src/newkit/**/*.ts' || true"
  },
  "repository": {
    "type": "git",
